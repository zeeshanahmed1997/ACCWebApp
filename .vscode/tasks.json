{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "StartWebApi",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"${workspaceFolder}/WebApi/WebApi.csproj"
			],
			"group": "build"
		},
		{
			"label": "StartMoonClothHous",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"${workspaceFolder}/MoonClothHous/MoonClothHous.csproj"
			],
			"group": "build"
		},
		{
			"label": "StartBothProjects",
			"dependsOrder": "parallel",
			"dependsOn": [
				"StartWebApi",
				"StartMoonClothHous"
			]
		}
	]
}